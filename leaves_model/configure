if model === 'vanilla'
  add leaves_model/vanilla/blockstates assets/minecraft/blockstates
  if ['Light','Dwarven'].includes(flavor)
    add leaves_model/vanilla/models/light_dwarven assets/minecraft/models/block/leaves/vanilla
  elseif ['High','Dark'].includes(flavor)
    add leaves_model/vanilla/models/high_dark assets/minecraft/models/block/leaves/vanilla
  endif
elseif model === 'bushy'
  add leaves_model/bushy_leaves assets/minecraft
end