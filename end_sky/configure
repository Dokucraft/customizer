if sky === 'vanilla' || sky === 'rift' || sky === 'cloudy'
  delete assets/minecraft/optifine/sky/world1
end
if sky === 'rift'
  scalar assets/minecraft/shaders/config.txt CUSTOM_END_SKY 1
  if typeof rift_color === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_RIFT_COLOR $rift_color
  end
  if typeof in_color === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_NEBULAE_INSIDE_COLOR $in_color
  end
  if typeof on_color === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_NEBULAE_OUTSIDE_COLOR $on_color
  end
  if typeof is_color === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_STARS_INSIDE_BASE_COLOR $is_color
  end
  if typeof os_color === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_STARS_OUTSIDE_BASE_COLOR $os_color
  end
elseif sky === 'cloudy'
  scalar assets/minecraft/shaders/config.txt CUSTOM_END_SKY 2
  if typeof c_base_color === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_2_BASE_COLOR $c_base_color
  end
  if typeof c_weak_color === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_2_WEAK_CLOUDS_COLOR $c_weak_color
  end
  if typeof c_strong_color === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_2_STRONG_CLOUDS_PRIMARY_COLOR $c_strong_color
  end
  if typeof c_strong_color_1 === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_2_STRONG_CLOUDS_COLOR_1 $c_strong_color_1
  end
  if typeof c_strong_color_2 === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_2_STRONG_CLOUDS_COLOR_2 $c_strong_color_2
  end
  if typeof c_stars_color === 'string'
    vec3 assets/minecraft/shaders/flavor.glsl END_SKY_2_STARS_BASE_COLOR $c_stars_color
  end
end