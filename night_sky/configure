if sky === 'procedural'
  if /1\.19\.2-Dokucraft-(Light|High|Dark|Dwarven)\.zip/.test(pack)
    enable assets/minecraft/shaders/program/skybox.fsh ENABLE_EXPERIMENTAL_PROCEDURAL_NIGHT_SKY
  else
    scalar assets/minecraft/shaders/program/skybox.fsh NIGHT_SKY 1
  end
end
if auroras
  enable assets/minecraft/shaders/program/skybox.fsh ENABLE_AURORAS
  if typeof aurora_color === 'string'
    vec3 assets/minecraft/shaders/program/skybox.fsh AURORA_COLOR $aurora_color
  end
end